{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier", "jest"],
  "globals": { "window": true, "document": true },
  "env": { "jest/globals": true },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "app/javascript/packs"]
      }
    }
  }
}
